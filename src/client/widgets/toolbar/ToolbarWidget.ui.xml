<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style src="styles/style.css" field="style"/>
    <ui:style src="styles/dynamic.css" field="dynamic" type="client.widgets.toolbar.ToolbarWidget.ToolbarStyles"/>
    <g:HTMLPanel>
        <div class="{style.toolbar}">
            <div class="{style.toolbar__left}">
                <div class="{style.title}">Title</div>
            </div>
            <div class="{style.toolbar__right}">
                <g:FlowPanel styleName="{style.login}" ui:field="emailField">
                    <g:Label ui:field="email"></g:Label>
                </g:FlowPanel>
                <div class="{style.useractions}" ui:field="userActionsList">
                    <g:HTMLPanel ui:field="logout"><div class="{style.useractions__action}">Выйти</div></g:HTMLPanel>
                </div>
            </div>
        </div>
    </g:HTMLPanel>

</ui:UiBinder>